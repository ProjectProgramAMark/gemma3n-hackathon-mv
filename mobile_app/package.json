{
  "name": "mobile_app",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest",
    "postinstall": "patch-package",
    "ios-development": "dotenv -e .env.development -- eas build --platform ios --profile development --local --clear-cache --non-interactive",
    "ios-preview": "dotenv -e .env.production -- eas build --platform ios --profile preview --local --clear-cache --non-interactive",
    "ios-production": "dotenv -e .env.production -- eas build --platform ios --profile production --local --clear-cache --non-interactive",
    "android-development": "dotenv -e .env.development -- eas build --platform android --profile development --clear-cache --non-interactive",
    "android-preview": "dotenv -e .env.production -- eas build --platform android --profile preview --clear-cache --non-interactive",
    "android-production": "dotenv -e .env.production -- eas build --platform android --profile production --clear-cache --non-interactive",
    "android-development-local": "dotenv -e .env.development -- eas build --platform android --profile development --local --clear-cache --non-interactive"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^4.6.1",
